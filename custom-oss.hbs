{{!< default}} 
<div id="open-source">
  {{#post}} 
    <div class="{{post_class}}">
      <h1 class="title">{{title}}</h1>
      <div class="two-up">{{content}}</div>
    </div>
  {{/post}}

  {{#get "posts" filter="slug:program-news" include="feature_image"}}
    {{#foreach posts}}
      <div class="{{post_class}} program-news">
        <div class="column">
          {{#if feature_image}}
            <img src="{{feature_image}}" alt="{{title}}" class="feature-image">
          {{/if}}
        </div>
        <div class="column">
          <h1>{{title}}</h1>
          {{{html}}}
        </div>
      </div>
    {{/foreach}}
  {{/get}}

  {{#get "posts" filter="slug:[our-vision, our-mission]"}}
    <div class="{{post_class}} mission-and-vision">
      {{#foreach posts}}
        <div class="column">
          <h1>{{title}}</h1>
          {{{html}}}
        </div>
      {{/foreach}}
    </div>
  {{/get}}
  <div class="projects">
  <div class="{{post_class}}">
    <h1 class="title">Projects</h1>
    <h2>Learn more about our current and past efforts</h2>

    
      <div class="projects-list">
      {{#get "posts" order="featured desc, title asc" filter="tag:[project]"}}
        {{#foreach posts}}
        <div class="{{post_class}} project">
            <h1>{{title}}</h1>
            {{{html}}}
        </div>
        {{/foreach}}
      {{/get}}
      </div>
      <div class="contribute">
        {{#get "posts" filter="slug:contribute"}}
        {{#foreach posts}}
        {{#if feature_image}}
        <img src="{{feature_image}}" alt="{{title}}" class="feature-image">
        {{/if}}
        <h1 class="title">{{title}}</h1>
        <div>{{content}}</div>
        {{/foreach}}
        {{/get}}
      </div>
    </div>
  </div>

  <div class="post" aria-labelledby="team-heading">
    <h1 id="team-heading" class="title">Team</h1>
    <h2>Meet the people behind our program</h2>
    <div class="grid">
      {{#get "pages" order="featured desc, title asc" filter="tags:hash-oss-team" limit="all" include="feature_image"}}
        {{#foreach pages}}
          <figure>
            {{#if feature_image}}
              <img src="{{feature_image}}" alt="{{title}}" class="feature-image team-photo">
            {{/if}}
            <figcaption class="team-info">
              <p class="team-name">{{title}}</p>
              {{content}}
            </figcaption>
          </figure>
        {{/foreach}}
      {{/get}}
    </div>
  </div>
</div>